<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.BarChartOfXRDelay"
            stylesheets="@css/seriesCharts.css"
            prefHeight="700.0" prefWidth="700.0">
    <BarChart  stylesheets="@css/barTR.css" fx:id="barChartTR" categoryGap="0.1"  prefHeight="320" AnchorPane.leftAnchor="30" AnchorPane.topAnchor="30" AnchorPane.rightAnchor="30">
        <xAxis>
            <CategoryAxis label="X-Ray delay, ns" />
        </xAxis>
        <yAxis>
            <NumberAxis label="TR Amplitude, mV" side="LEFT" autoRanging="true" upperBound="1000" tickUnit="250"/>
        </yAxis>
    </BarChart>
    <BarChart fx:id="barChartIntegral" legendVisible="false" categoryGap="0.1" prefHeight="320" AnchorPane.leftAnchor="30" AnchorPane.rightAnchor="30" AnchorPane.bottomAnchor="30">
        <xAxis>
            <CategoryAxis label="X-Ray delay, ns" />
        </xAxis>
        <yAxis>
            <NumberAxis label="X-Ray Integral" side="LEFT" autoRanging="true" upperBound="500" tickUnit="100"/>
        </yAxis>
    </BarChart>
</AnchorPane>
